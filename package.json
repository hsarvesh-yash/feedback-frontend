{
  "name": "frontend",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:restart": "nodemon --watch src --watch public --ext ts,tsx,js,jsx,css,html,wasm --exec \"vite\" --signal SIGINT",
    "build": "tsc -b && vite build && node ./scripts/bump-version.js minor && git add package.json && git commit -m \"chore: bump minor version (build)\" || true",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.2.1",
    "lucide-react": "^0.575.0",
    "sql.js": "^1.8.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.1",
    "recharts": "^3.7.0",
    "tailwindcss": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "nodemon": "^3.0.1",
    "husky": "^8.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^8.0.0-beta.13"
  },
  "prepare": "husky install",
  "overrides": {
    "vite": "^8.0.0-beta.13"
  }
}
